# Shortcuts
alias g='git'

# Git stuff
alias gs="git status"
alias gd="git diff"
alias gds="git diff --staged"
alias gl="git log --graph --pretty=format:'%C(yellow)%h%Creset  %C(cyan)%d%Creset %C(bold)%s%Creset %C(dim)(%cr)%Creset' --abbrev-commit"
alias gll="git log --graph --pretty=format:'%C(yellow)%h%Creset  %Cgreen%an%Creset %C(cyan)%d%Creset %C(bold)%s%Creset  %C(dim)(%cD)%Creset' --abbrev-commit --all"

# GO
alias gb="go build -v"
alias gt="go test -cover ./..."
