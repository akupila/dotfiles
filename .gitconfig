[user]
	name = akupila
	email = akupila@gmail.com
[mergetool]
	keepBackup = false
[color]
	ui = auto
[branch]
	autosetupmerge = true
[merge]
	stat = true
	tool = opendiff
[core]
	ignorecase = false
	editor = /usr/local/bin/vim
	pager = diff-so-fancy | less --tabs=4 -RFX
	excludesfile = ~/.gitignore
[diff]
	compactionHeuristic = true
[diff-so-fancy]
[pull]
	rebase = true
[rebase]
	autosquash = true
[grep]
	lineNumber = true
